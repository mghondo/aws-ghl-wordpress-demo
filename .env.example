# AWS S3 Settings
AWS_S3_BUCKET_NAME=clarity-aws-ghl-demo-storage
AWS_S3_REGION=us-east-1
AWS_S3_ACCESS_KEY_ID=your_actual_access_key_here
AWS_S3_SECRET_ACCESS_KEY=your_actual_secret_key_here

# S3 Configuration Options
AWS_S3_DELETE_LOCAL_FILES=false
AWS_S3_UPLOAD_PATH=uploads/

# GoHighLevel Configuration
GHL_WEBHOOK_SECRET=your_webhook_secret_here
GHL_WEBHOOK_ENABLED=true

# WordPress Debug (helpful for local development)
WP_DEBUG=true
WP_DEBUG_LOG=true